@model Blog.Models.Post

<tr>
    <td><input name="DeleteImages" value="@Model.Id" id="checkbox-@Model.Id" type="checkbox" /></td>
    <td>
        <a href="#@Model.Id" title="Edit @Model.Title">
            <img src="~/uploads/thumb/@Model.Guid" alt="@Model.Title" height="60" />
        </a>
    </td>
    <td class="row-title"><a href="@Url.Action("edit", new {Model.Id})">@Model.Title</a></td>
    <td>@Model.User.UserName</td>
    <td>@Model.CreateAt</td>    
</tr>

